{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "#Section",
    "title": "Section",
    "type": "object",
    "properties": {
        "verticalPosition": {
            "type": "string",
            "default": "center"
        },
        "addTopOffset": {
            "type": "number"
        },
        "justifyContent": {
            "type": "string",
            "default": "space-between"
        },
        "alignItems": {
            "type": "string",
            "default": "center"
        }
    }
}