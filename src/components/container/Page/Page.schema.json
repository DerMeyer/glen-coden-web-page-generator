{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "#Page",
    "title": "Page",
    "type": "object",
    "properties": {
        "rows": {
            "type": "number",
            "default": 3
        },
        "minHeight": {
            "type": "number",
            "default": 0
        },
        "color": {
            "type": "string",
            "default": "background"
        },
        "children": {
            "type": "array",
            "items": {
                "type": "object"
            },
            "default": [
                {
                    "component": "Row",
                    "alignSelf": "start"
                },
                {
                    "component": "Row",
                    "alignSelf": "center"
                },
                {
                    "component": "Row",
                    "alignSelf": "end"
                }
            ]
        }
    }
}