{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "#ViewportBox",
    "title": "ViewportBox",
    "type": "object",
    "properties": {
        "width": {
            "type": "number",
            "default": 100
        },
        "height": {
            "type": "number",
            "default": 100
        },
        "center": {
            "type": "boolean",
            "default": false
        },
        "children": {
            "type": "array",
            "items": {
                "type": "object"
            }
        }
    }
}