{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "#LoadingOverlay",
    "title": "LoadingOverlay",
    "type": "object",
    "properties": {
        "color": {
            "type": "string"
        },
        "bg": {
            "type": "string"
        },
        "fromGlobal": {
            "type": "array",
            "items": {
                "type": [ "string", "object" ]
            },
            "default": [
                "fadeInTime"
            ]
        }
    }
}