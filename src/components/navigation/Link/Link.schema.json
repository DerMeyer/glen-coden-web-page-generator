{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "#Link",
    "title": "Link",
    "type": "object",
    "properties": {
        "external": {
            "type": "boolean",
            "default": false
        },
        "target": {
            "type": "string",
            "default": "/"
        },
        "icon": {
            "type": "string",
            "default": ""
        },
        "iconSize": {
            "type": "number",
            "default": 40
        },
        "css": {
            "type": "object",
            "default": {}
        }
    }
}