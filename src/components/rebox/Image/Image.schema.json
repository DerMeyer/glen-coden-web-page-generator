{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "#Image",
    "title": "Image",
    "type": "object",
    "properties": {
        "source": {
            "type": "string"
        },
        "ratio": {
            "type": "number",
            "default": 1.5
        },
        "subscribeToGlobalLoading": {
            "type": "boolean",
            "default": false
        },
        "loadAfterGlobalLoading": {
            "type": "boolean",
            "default": false
        }
    }
}