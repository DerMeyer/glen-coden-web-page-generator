{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "#ZoomBackground",
    "title": "ZoomBackground",
    "type": "object",
    "properties": {
        "image": {
            "type": "string"
        },
        "auto": {
            "type": "boolean",
            "default": [ true, false ]
        },
        "factor": {
            "type": "number",
            "default": 4
        },
        "time": {
            "type": "number",
            "default": 0.5
        }
    }
}